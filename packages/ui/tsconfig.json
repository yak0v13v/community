{
  "compilerOptions": {
    "target": "es5",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "include": ["ui.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
